function twoSum(array,targetValue){
   for(let i=0;i<array.length-1;i++){
       let firstNum=array[i]
       for(let j=i+1;i<array.length;j++){
       
           let second=array[j]
           if(firstNum+second==targetValue){
            return [firstNum,second]
           }
   }
   }
   return []
}

let a=twoSum([2,7,11,15],9)
console.log(a)
